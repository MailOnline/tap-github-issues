{
  "name": "tap-github-issues",
  "version": "0.0.1",
  "description": "Test reporter that converts TAP output into github issues in multiple orgs/repos",
  "main": "index.js",
  "bin": {
    "tap-github-issues": "lib/cli/index.js"
  },
  "scripts": {
    "eslint": "eslint lib && eslint spec",
    "test-spec": "mocha 'spec/{**/,}*.spec.js' -R spec",
    "test": "npm run eslint && nyc npm run test-spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MailOnline/tap-github-issues.git"
  },
  "keywords": [
    "tap",
    "github",
    "issues",
    "reporter"
  ],
  "author": "MailOnline",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MailOnline/tap-github-issues/issues"
  },
  "homepage": "https://github.com/MailOnline/tap-github-issues#readme",
  "dependencies": {
    "co": "^4.6.0",
    "minimist": "^1.2.0",
    "moment": "^2.18.1",
    "request": "^2.81.0",
    "request-promise": "^4.2.0",
    "tap-parser": "git+ssh://git@github.com/MailOnline/tap-parser"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "mocha": "^3.2.0",
    "nock": "^9.0.13",
    "nyc": "^10.2.0"
  },
  "nyc": {
    "exclude": [
      "spec"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  }
}
